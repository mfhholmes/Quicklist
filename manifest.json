{
  "name": "Quicklist",
  "description": "A simple chrome extension that stores links and notes",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "quicklist.html",
    "default_icon":"logo19.png",
    "default_title":"Quicklist"
  },
  "icons": { "16": "logo16.png",
						"32":"logo32.png",
           "48": "logo48.png",
           "64":"logo64.png",
          "128": "logo128.png" },
  "permissions": [
    "storage",
    "tabs"
  ]
}